    <h4 class="widget_title">Danh mục tin tức</h4>
    <ul class="list cat-list">

        @if(Model != null && Model.Count() > 0)
        {
            foreach(var item in Model)
            {
                <li>
                    <a href="#"  class="d-flex justify-content-between">
